# Configurações do Ambiente de Produção
ENVIRONMENT=production
DEBUG=false

# Supabase Configuration (Production)
SUPABASE_URL_PRODUCTION=https://your-production-project.supabase.co
SUPABASE_ANON_KEY_PRODUCTION=your-production-anon-key
SUPABASE_SERVICE_KEY_PRODUCTION=your-production-service-key

# Database Configuration (Production)
DATABASE_URL_PRODUCTION=postgresql://postgres:password@localhost:5432/milapp_production

# Redis Configuration (Production)
REDIS_URL_PRODUCTION=redis://localhost:6379/0

# Security (Production)
SECRET_KEY_PRODUCTION=your-production-secret-key-32-chars-long-with-special-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Rate Limiting (Production)
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# Azure AD Configuration (Production)
AZURE_TENANT_ID=your-azure-tenant-id
AZURE_CLIENT_ID=your-azure-client-id
AZURE_CLIENT_SECRET=your-azure-client-secret

# AI Services
OPENAI_API_KEY=your-openai-api-key
LANGCHAIN_API_KEY=your-langchain-api-key

# External Integrations
N8N_BASE_URL=https://n8n.your-domain.com
N8N_API_KEY=your-n8n-api-key

# Notifications
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# File Storage (MinIO/S3) - Production
MINIO_ENDPOINT_PRODUCTION=localhost:9000
MINIO_ACCESS_KEY_PRODUCTION=minioadmin-production
MINIO_SECRET_KEY_PRODUCTION=minioadmin-production
MINIO_BUCKET_NAME_PRODUCTION=milapp-files-production
MINIO_SECURE=false

# Monitoring (Production)
PROMETHEUS_URL_PRODUCTION=http://localhost:9090
GRAFANA_URL_PRODUCTION=http://localhost:3001

# CORS (Production)
CORS_ORIGINS_PRODUCTION=https://milapp.com,https://www.milapp.com

# Grafana Admin Password (Production)
GRAFANA_ADMIN_PASSWORD=your-secure-production-password 