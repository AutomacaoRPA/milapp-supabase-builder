# Configurações do Ambiente de Staging/Demo
ENVIRONMENT=staging
DEBUG=true

# Supabase Configuration (Staging)
SUPABASE_URL_STAGING=https://your-staging-project.supabase.co
SUPABASE_ANON_KEY_STAGING=your-staging-anon-key
SUPABASE_SERVICE_KEY_STAGING=your-staging-service-key

# Database Configuration (Staging)
DATABASE_URL_STAGING=postgresql://postgres:password@localhost:5432/milapp_staging

# Redis Configuration (Staging)
REDIS_URL_STAGING=redis://localhost:6380/0

# Security (Staging)
SECRET_KEY_STAGING=your-staging-secret-key-32-chars-long-with-special-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Rate Limiting (Staging)
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# Azure AD Configuration (Staging)
AZURE_TENANT_ID=your-azure-tenant-id
AZURE_CLIENT_ID=your-azure-client-id
AZURE_CLIENT_SECRET=your-azure-client-secret

# AI Services
OPENAI_API_KEY=your-openai-api-key
LANGCHAIN_API_KEY=your-langchain-api-key

# External Integrations
N8N_BASE_URL=https://n8n-staging.your-domain.com
N8N_API_KEY=your-n8n-api-key

# Notifications
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# File Storage (MinIO/S3) - Staging
MINIO_ENDPOINT_STAGING=localhost:9002
MINIO_ACCESS_KEY_STAGING=minioadmin-staging
MINIO_SECRET_KEY_STAGING=minioadmin-staging
MINIO_BUCKET_NAME_STAGING=milapp-files-staging
MINIO_SECURE=false

# Monitoring (Staging)
PROMETHEUS_URL_STAGING=http://localhost:9091
GRAFANA_URL_STAGING=http://localhost:3002

# CORS (Staging)
CORS_ORIGINS_STAGING=http://localhost:3001,http://staging.milapp.com

# Grafana Admin Password (Staging)
GRAFANA_ADMIN_PASSWORD=admin-staging 